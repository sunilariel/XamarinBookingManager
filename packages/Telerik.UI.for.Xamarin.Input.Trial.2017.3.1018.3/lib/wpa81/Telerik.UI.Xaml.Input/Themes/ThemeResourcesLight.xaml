<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:telerik="using:Telerik.UI.Xaml.Controls">

    <!--#region SegmentedControl Brushes-->

    <Color x:Key="TelerikSegmentedControlDisabledColor">#FFD9D9D9</Color>

    <SolidColorBrush x:Key="TelerikSegmentedControlBackgroundThemeBrush" Color="Transparent" />
    <SolidColorBrush x:Key="TelerikSegmentedControlBorderThemeBrush" Color="{ThemeResource TelerikSelectedColor}" />
    <SolidColorBrush x:Key="TelerikSegmentedControlSeparatorThemeBrush" Color="{ThemeResource TelerikSelectedColor}" />

    <SolidColorBrush x:Key="TelerikSegmentedControlDisabledBackgroundThemeBrush" Color="Transparent" />
    <SolidColorBrush x:Key="TelerikSegmentedControlDisabledBorderThemeBrush" Color="{ThemeResource TelerikSegmentedControlDisabledColor}" />
    <SolidColorBrush x:Key="TelerikSegmentedControlDisabledSeparatorThemeBrush" Color="{ThemeResource TelerikSegmentedControlDisabledColor}" />

    <!--segment normal-->

    <SolidColorBrush x:Key="TelerikSegmentBackgroundThemeBrush" Color="Transparent" />
    <SolidColorBrush x:Key="TelerikSegmentContentForegroundThemeBrush" Color="{ThemeResource TelerikForegroundColor}" />
    <SolidColorBrush x:Key="TelerikSegmentBorderThemeBrush" Color="Transparent" />

    <!--segment pointer over-->

    <SolidColorBrush x:Key="TelerikSegmentPointerOverBackgroundThemeBrush" Color="{ThemeResource TelerikSelectedColor}" Opacity="0.4" />
    <SolidColorBrush x:Key="TelerikSegmentPointerOverContentForegroundThemeBrush" Color="{ThemeResource TelerikForegroundColor}" />
    <SolidColorBrush x:Key="TelerikSegmentPointerOverBorderThemeBrush" Color="Transparent" />

    <!--segment pressed-->

    <SolidColorBrush x:Key="TelerikSegmentPressedBackgroundThemeBrush" Color="{ThemeResource TelerikSelectedColor}" />
    <SolidColorBrush x:Key="TelerikSegmentPressedContentForegroundThemeBrush" Color="{ThemeResource TelerikPressedForegroundColor}" />
    <SolidColorBrush x:Key="TelerikSegmentPressedBorderThemeBrush" Color="Transparent" />

    <!--segment disabled-->

    <SolidColorBrush x:Key="TelerikSegmentDisabledBackgroundThemeBrush" Color="Transparent" />
    <SolidColorBrush x:Key="TelerikSegmentDisabledContentForegroundThemeBrush" Color="{ThemeResource TelerikSegmentedControlDisabledColor}" />
    <SolidColorBrush x:Key="TelerikSegmentDisabledBorderThemeBrush" Color="Transparent" />

    <!--segment selected-->

    <SolidColorBrush x:Key="TelerikSegmentSelectedBackgroundThemeBrush" Color="{ThemeResource TelerikSelectedColor}" />
    <SolidColorBrush x:Key="TelerikSegmentSelectedContentForegroundThemeBrush" Color="{ThemeResource TelerikPressedForegroundColor}" />
    <SolidColorBrush x:Key="TelerikSegmentSelectedBorderThemeBrush" Color="Transparent" />

    <!--segment selected pointer over-->

    <SolidColorBrush x:Key="TelerikSegmentSelectedPointerOverBackgroundThemeBrush" Color="{ThemeResource TelerikSelectedColor}" Opacity="0.7" />
    <SolidColorBrush x:Key="TelerikSegmentSelectedPointerOverContentForegroundThemeBrush" Color="{ThemeResource TelerikPressedForegroundColor}" />
    <SolidColorBrush x:Key="TelerikSegmentSelectedPointerOverBorderThemeBrush" Color="Transparent" />

    <!--segment selected pressed-->

    <SolidColorBrush x:Key="TelerikSegmentSelectedPressedBackgroundThemeBrush" Color="{ThemeResource TelerikSelectedColor}" />
    <SolidColorBrush x:Key="TelerikSegmentSelectedPressedContentForegroundThemeBrush" Color="{ThemeResource TelerikPressedForegroundColor}" />
    <SolidColorBrush x:Key="TelerikSegmentSelectedPressedBorderThemeBrush" Color="Transparent" />

    <!--segment selected disabled-->

    <SolidColorBrush x:Key="TelerikSegmentSelectedDisabledBackgroundThemeBrush" Color="{ThemeResource TelerikSegmentedControlDisabledColor}" />
    <SolidColorBrush x:Key="TelerikSegmentSelectedDisabledContentForegroundThemeBrush" Color="{ThemeResource TelerikWhiteColor}" />
    <SolidColorBrush x:Key="TelerikSegmentSelectedDisabledBorderThemeBrush" Color="Transparent" />

    <!--#endregion-->

    <!--#region AutoCompleteBox Brushes-->

    <!--Inline-->

    <SolidColorBrush x:Key="TelerikAutoCompleteBoxForegroundBrush" Color="{StaticResource TelerikBlackColor}" />

    <SolidColorBrush x:Key="TelerikAutoCompleteBoxBackgroundBrush" Color="#CCFFFFFF" />
    <SolidColorBrush x:Key="TelerikAutoCompleteBoxBorderBrush" Color="#45000000" />

    <SolidColorBrush x:Key="TelerikAutoCompleteBoxHeaderForegroundBrush" Color="{StaticResource TelerikSecondaryForegroundColor}" />
    <SolidColorBrush x:Key="TelerikAutoCompleteBoxWatermarkForegroundBrush" Color="{StaticResource TelerikWatermarkForegroundColor}" />

    <!--PointerOver-->
    <SolidColorBrush x:Key="TelerikAutoCompleteBoxPointerOverBackgroundBrush" Color="#DEFFFFFF" />
    <SolidColorBrush x:Key="TelerikAutoCompleteBoxPointerOverBorderBrush" Color="#70000000" />

    <!--Focused-->
    <SolidColorBrush x:Key="TelerikAutoCompleteBoxFocusedForegroundBrush" Color="{StaticResource TelerikWhiteColor}" />
    <SolidColorBrush x:Key="TelerikAutoCompleteBoxFocusedBackgroundBrush" Color="{StaticResource TelerikWhiteColor}" />
    <SolidColorBrush x:Key="TelerikAutoCompleteBoxFocusedBorderBrush" Color="#A3000000" />

    <!--Disabled-->
    <SolidColorBrush x:Key="TelerikAutoCompleteBoxDisabledForegroundBrush" Color="{StaticResource TelerikDisabledColor}" />
    <SolidColorBrush x:Key="TelerikAutoCompleteBoxDisabledBackgroundBrush" Color="#66CACACA" />
    <SolidColorBrush x:Key="TelerikAutoCompleteBoxDisabledBorderBrush" Color="{StaticResource TelerikDisabledColor}" />
    <SolidColorBrush x:Key="TelerikAutoCompleteBoxWatermarkDisabledForegroundBrush" Color="{StaticResource TelerikWatermarkDisabledForegroundColor}" />

    <!--Selector-->
    <SolidColorBrush x:Key="TelerikAutoCompleteBoxSelectorBackgroundBrush" Color="{StaticResource TelerikWhiteColor}" />
    <SolidColorBrush x:Key="TelerikAutoCompleteBoxSelectorBorderBrush" Color="#FF212121" />

    <SolidColorBrush x:Key="TelerikAutoCompleteBoxSelectorItemBorderBrush" Color="#FF212121" />
    <SolidColorBrush x:Key="TelerikAutoCompleteBoxSelectorSelectedItemBackgroundBrush" Color="{StaticResource TelerikSelectedColor}" />
    <SolidColorBrush x:Key="TelerikAutoCompleteBoxSelectorSelectedItemBorderBrush" Color="#FF212121" />

    <!--#endregion-->

    <!--#region DateTimePicker Brushes-->

    <!--Header-->
    <SolidColorBrush x:Key="TelerikDateTimePickerHeaderForegroundBrush" Color="{StaticResource PhoneBaseMidColor}" />

    <!--Rest state-->
    <SolidColorBrush x:Key="TelerikDateTimePickerBackgroundBrush" Color="Transparent" />
    <SolidColorBrush x:Key="TelerikDateTimePickerBorderBrush" Color="{StaticResource PhoneForegroundColor}" />
    <SolidColorBrush x:Key="TelerikDateTimePickerForegroundBrush" Color="{StaticResource PhoneForegroundColor}" />

    <!--Pressed state-->
    <SolidColorBrush x:Key="TelerikDateTimePickerPressedBackgroundBrush" Color="{StaticResource SystemColorControlAccentColor}" />
    <SolidColorBrush x:Key="TelerikDateTimePickerPressedBorderBrush" Color="{StaticResource PhoneForegroundColor}" />
    <SolidColorBrush x:Key="TelerikDateTimePickerPressedForegroundBrush" Color="{StaticResource PhoneTextOverAccentColor}" />

    <!--Disabled state-->
    <SolidColorBrush x:Key="TelerikDateTimePickerDisabledBackgroundBrush" Color="Transparent" />
    <SolidColorBrush x:Key="TelerikDateTimePickerDisabledBorderBrush" Color="{ThemeResource PhoneBaseLowColor}" />
    <SolidColorBrush x:Key="TelerikDateTimePickerDisabledForegroundBrush" Color="{ThemeResource PhoneBaseLowColor}" />

    <!--Selector-->
    <SolidColorBrush x:Key="TelerikDateTimePickerSelectorBackgroundBrush" Color="{ThemeResource PhoneChromeColor}" />
    <SolidColorBrush x:Key="TelerikDateTimePickerSelectorForegroundBrush" Color="{ThemeResource PhoneForegroundColor}" />

    <!--SelectorItem Rest State-->
    <SolidColorBrush x:Key="TelerikDateTimePickerSelectorItemBackgroundBrush" Color="Transparent" />
    <SolidColorBrush x:Key="TelerikDateTimePickerSelectorItemBorderBrush" Color="{ThemeResource PhoneBaseLowColor}" />
    <SolidColorBrush x:Key="TelerikDateTimePickerSelectorItemForegroundBrush" Color="{ThemeResource PhoneBaseLowColor}" />

    <!--SelectorItem Select State-->
    <SolidColorBrush x:Key="TelerikDateTimePickerSelectorItemSelectedBackgroundBrush" Color="{ThemeResource SystemColorControlAccentColor}" />
    <SolidColorBrush x:Key="TelerikDateTimePickerSelectorItemSelectedBorderBrush" Color="{ThemeResource SystemColorControlAccentColor}" />
    <SolidColorBrush x:Key="TelerikDateTimePickerSelectorItemSelectedForegroundBrush" Color="{ThemeResource PhoneTextOverAccentColor}" />

    <!--SelectorItem Disabled State-->
    <SolidColorBrush x:Key="TelerikDateTimePickerSelectorItemDisabledBackgroundBrush" Color="Transparent" />
    <SolidColorBrush x:Key="TelerikDateTimePickerSelectorItemDisabledBorderBrush" Color="#FFCCCCCC" />
    <SolidColorBrush x:Key="TelerikDateTimePickerSelectorItemDisabledForegroundBrush" Color="#FFCCCCCC" />

    <!--#endregion-->

    <!--#region NumericBox Brushes-->

    <!--Normal State-->
    <SolidColorBrush x:Key="TelerikNumericBoxBackgroundBrush" Color="#FFFFFFFF" />
    <SolidColorBrush x:Key="TelerikNumericBoxForegroundBrush" Color="{StaticResource TelerikBlackColor}" />
    <SolidColorBrush x:Key="TelerikNumericBoxBorderBrush" Color="#45000000" />
    <SolidColorBrush x:Key="TelerikNumericBoxHeaderForegroundBrush" Color="{StaticResource TelerikSecondaryForegroundColor}" />
    <SolidColorBrush x:Key="TelerikNumericBoxWatermarkForegroundBrush" Color="{StaticResource TelerikWatermarkForegroundColor}" />

    <!--PointerOver State-->
    <SolidColorBrush x:Key="TelerikNumericBoxPointerOverBackgroundBrush" Color="#FFFFFFFF" />
    <SolidColorBrush x:Key="TelerikNumericBoxPointerOverBorderBrush" Color="#70000000" />

    <!--Focused State-->
    <SolidColorBrush x:Key="TelerikNumericBoxFocusedBackgroundBrush" Color="{StaticResource TelerikWhiteColor}" />
    <SolidColorBrush x:Key="TelerikNumericBoxFocusedBorderBrush" Color="#A3000000" />

    <!--Disabled State-->
    <SolidColorBrush x:Key="TelerikNumericBoxDisabledBackgroundBrush" Color="#66CACACA" />
    <SolidColorBrush x:Key="TelerikNumericBoxDisabledForegroundBrush" Color="{StaticResource TelerikWatermarkForegroundColor}" />
    <SolidColorBrush x:Key="TelerikNumericBoxDisabledBorderBrush" Color="#26000000" />
    <SolidColorBrush x:Key="TelerikNumericBoxWatermarkDisabledForegroundBrush" Color="{StaticResource TelerikWatermarkDisabledForegroundColor}" />

    <!--Spin Button-->
    <SolidColorBrush x:Key="TelerikNumericBoxSpinButtonBackgroundBrush" Color="{StaticResource TelerikSelectedColor}" />
    <SolidColorBrush x:Key="TelerikNumericBoxSpinButtonForegroundBrush" Color="#FFFFFFFF" />
    <SolidColorBrush x:Key="TelerikNumericBoxSpinButtonPointerOverBackgroundBrush" Color="#FF217DC9" />
    <SolidColorBrush x:Key="TelerikNumericBoxSpinButtonPressedBackgroundBrush" Color="{StaticResource TelerikForegroundColor}" />
    <SolidColorBrush x:Key="TelerikNumericBoxSpinButtonPressedForegroundBrush" Color="#FFFFFFFF" />
    <SolidColorBrush x:Key="TelerikNumericBoxSpinButtonDisabledBackgroundBrush" Color="#66CACACA" />
    <SolidColorBrush x:Key="TelerikNumericBoxSpinButtonDisabledForegroundBrush" Color="{StaticResource TelerikDisabledColor}" />
    <SolidColorBrush x:Key="TelerikNumericBoxSpinButtonDisabledBorderBrush" Color="#26000000" />

    <!--Delete Button-->
    <SolidColorBrush x:Key="TelerikNumericBoxDeleteButtonForegroundBrush" Color="{StaticResource TelerikBlackColor}" />
    <SolidColorBrush x:Key="TelerikNumericBoxDeleteButtonPointerOverBackgroundBrush" Color="#C4CDCDCD" />
    <SolidColorBrush x:Key="TelerikNumericBoxDeleteButtonPressedBackgroundBrush" Color="{StaticResource TelerikBlackColor}" />
    <SolidColorBrush x:Key="TelerikNumericBoxDeleteButtonPressedForegroundBrush" Color="{StaticResource TelerikWhiteColor}" />

    <!--#endregion-->

    <!--#region Calendar Brushes-->

    <!--GridLines-->
    <SolidColorBrush x:Key="TelerikCalendarGridLinesBrush" Color="#FF000000" />

    <!--DayNames-->
    <SolidColorBrush x:Key="TelerikCalendarDayNamesForegroundBrush" Color="#FF767676" />

    <!--CellStyles-->
    <SolidColorBrush x:Key="TelerikCalendarNormalCellForegroundBrush" Color="#FF767676" />
    <SolidColorBrush x:Key="TelerikCalendarAnotherViewCellForegroundBrush" Color="#99000000" />

    <SolidColorBrush x:Key="TelerikCalendarBlackoutCellForegroundBrush" Color="#FF767676" />
    <SolidColorBrush x:Key="TelerikCalendarBlackoutCellBackgroundBrush" Color="#00FFFFFF" />

    <SolidColorBrush x:Key="TelerikCalendarSelectedCellForegroundBrush" Color="#FF000000" />
    <SolidColorBrush x:Key="TelerikCalendarSelectedCellBackgroundBrush" Color="#33000000" />
    <SolidColorBrush x:Key="TelerikCalendarSelectedCellBorderBrush" Color="#00FFFFFF" />

    <SolidColorBrush x:Key="TelerikCalendarHighlightedCellBorderBrush" Color="#FF3399FF" />

    <!--HoldClueControl-->
    <SolidColorBrush x:Key="TelerikCalendarHoldClueControlBackgroundBrush" Color="#FF3399FF" />

    <!--NavigationControl-->
    <SolidColorBrush x:Key="TelerikCalendarNavigationControlBackgroundBrush" Color="#00FFFFFF" />

    <SolidColorBrush x:Key="TelerikCalendarPreviousNextButtonForegroundBrush" Color="#FF767676" />
    <SolidColorBrush x:Key="TelerikCalendarPreviousNextButtonDisabledForegroundBrush" Color="#FFCCCCCC" />
    <SolidColorBrush x:Key="TelerikCalendarPreviousNextButtonPointerOverForegroundBrush" Color="#FF3399FF" />
    <SolidColorBrush x:Key="TelerikCalendarPreviousNextButtonPointerPressedForegroundBrush" Color="#FF3399FF" />

    <SolidColorBrush x:Key="TelerikCalendarHeaderButtonForegroundBrush" Color="{StaticResource TelerikBlackColor}" />
    <SolidColorBrush x:Key="TelerikCalendarHeaderButtonDisabledForegroundBrush" Color="#FF767676" />
    <SolidColorBrush x:Key="TelerikCalendarHeaderButtonPointerPressedForegroundBrush" Color="#FF3399FF" />
    <SolidColorBrush x:Key="TelerikCalendarHeaderButtonPointerOverForegroundBrush" Color="#FF3399FF" />

    <!--Hover-->
    <SolidColorBrush x:Key="TelerikCalendarPointerOverBackgroundBrush" Color="#193399FF" />
    <SolidColorBrush x:Key="TelerikCalendarPointerOverBorderBrush" Color="#FF3399FF" />

    <SolidColorBrush x:Key="TelerikCalendarCurrentCellBorderBrush" Color="#FF0066CC" />

    <!--#endregion-->

    <telerik:UserThemeResources x:Key="ResourceLoaderInitializer" />

    <ResourceDictionary.MergedDictionaries>
        <ResourceDictionary Source="ms-appx:///Telerik.UI.Xaml.Primitives/Themes/ThemeColorsLight.xaml" />
        <ResourceDictionary Source="{CustomResource LightResourcesPath}" />
    </ResourceDictionary.MergedDictionaries>
</ResourceDictionary>

